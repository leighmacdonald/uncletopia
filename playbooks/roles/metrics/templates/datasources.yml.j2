apiVersion: 1
prune: true

deleteDatasources:
  - name: "PostgresSQL (gbans)"
    uid: gbans-postgres
  - name: "PostgresSQL (bd-api)"
    uid: bdapi
  - name: "Prometheus"

datasources:
  - name: "PostgresSQL (gbans)"
    type: postgres
    url: gbans-postgres:5432
    access: proxy
    uid: gbans-postgres
    user: gbans
    jsonData:
      database: gbans
      sslmode: 'disable' # disable/require/verify-ca/verify-full
      maxOpenConns: 0 # Grafana v5.4+
      maxIdleConns: 2 # Grafana v5.4+
      connMaxLifetime: 14400 # Grafana v5.4+
      postgresVersion: 10
      timescaledb: false
    secureJsonData:
      password: gbans
    version: 2
    editable: true

  - name: "PostgresSQL (bd-api)"
    type: postgres
    url: bdapi-postgres:5432
    access: proxy
    uid: bdapi-postgres
    user: bdapi
    jsonData:
      database: bdapi
      sslmode: 'disable' # disable/require/verify-ca/verify-full
      maxOpenConns: 0 # Grafana v5.4+
      maxIdleConns: 2 # Grafana v5.4+
      connMaxLifetime: 14400 # Grafana v5.4+
      postgresVersion: 10
      timescaledb: true
    secureJsonData:
      password: bdapi
    version: 2
    editable: true

