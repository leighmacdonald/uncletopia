apiVersion: 1

datasources:
  - name: PostgresSQL
    type: postgres
    url: gbans-postgres:5432
    access: proxy
    uid: gbans-postgres
    jsonData:
      database: grafana
      sslmode: 'disable' # disable/require/verify-ca/verify-full
      maxOpenConns: 0 # Grafana v5.4+
      maxIdleConns: 2 # Grafana v5.4+
      connMaxLifetime: 14400 # Grafana v5.4+
      postgresVersion: 10
      timescaledb: false
    secureJsonData:
      password: gbans
    version: 2
    editable: false

  - name: Prometheus
    type: prometheus
    uid: prometheus
    access: proxy
    url: http://prometheus:9090
    jsonData:
      httpMethod: POST
      manageAlerts: false
      prometheusType: Prometheus
      prometheusVersion: 2.40.0

    version: 1
    editable: false
