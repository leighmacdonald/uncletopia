// Show current vote progress as chat messages
// -
// Default: "0"
// Minimum: "0.000000"
// Maximum: "1.000000"
nativevotes_progress_chat "{{ nativevotes_progress_chat|default(0) }}"

// Show current vote progress as console messages to clients
// -
// Default: "0"
// Minimum: "0.000000"
// Maximum: "1.000000"
nativevotes_progress_client_console "{{ nativevotes_progress_client_console|default(0) }}"

// Show current vote progress as console messages
// -
// Default: "0"
// Minimum: "0.000000"
// Maximum: "1.000000"
nativevotes_progress_console "{{ nativevotes_progress_console|default(1) }}"

// Show current vote progress in a hint box
// -
// Default: "0"
// Minimum: "0.000000"
// Maximum: "1.000000"
nativevotes_progress_hintbox "{{ nativevotes_progress_hintbox|default(1) }}"

// Sets the recommended time in between public votes
// -
// Default: "30"
// Minimum: "0.000000"
nativevotes_vote_delay "{{ nativevotes_vote_delay|default(30) }}"
